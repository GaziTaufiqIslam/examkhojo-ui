extends templates/_default

block title
	title Pattern Library | ExamKhojo

block stylesheet
	link(rel="stylesheet" href="assets/css/pages/library.css")

block content
	include templates/partials/_nav
	.header
		.container
			h2.header-02 Pattern Library
			a(href="/index.html") Back

	section.typography
		.container
			.typography-title
				h2.header-02 Typography
			.typography-content
				.typeface
					.typeface-label
						h5.sub-header-02 Typeface
					.typeface-content
						h3.header-03 Public Sans
						p.sub-header-02 abcd efgh ijklmnop qrst uvwxyz
						p.sub-header-02 ABCD EFGJ IJKLMNOP QRST UVWXYZ
						p.sub-header-02 0123456789 (!@#%&.,?:;)
				
				.text-styles
					- 
						const textStyles = {
							"CLASS NAME" : [ "TAG", "FONT-WEIGHT", "EXAMPLE"],
							"header-01" : [ "h1", "800", "Header 01"],
							"header-02" : [ "h2", "700", "Header 02"],
							"header-03" : [ "h3", "600", "Header 03"],
							"sub-header-01" : [ "h4", "600", "Sub Header 01"],
							"sub-header-02" : [ "h5", "400", "Sub Header 02"],
							"sub-header-03" : [ "h6", "600", "sub Header 03"],
							"paragraph-01" : [ "p", "300", "Paragraph 01"],
							"button-01" : [ "a", "500", "Button 01"],
							"link-01" : [ "a", "500", "Link 01"],
							"label-01" : [ "span", "400", "Label 01"],
							"label-02" : [ "span", "400", "Label 02"]
						};
					.text-styles_content
						each n, i in textStyles
							.text-styles-each
								p.text-styles-each_class= i
								p.text-styles-each_tag= n[0]
								p.text-styles-each_weight= n[1]
								#{ n[0]  }.text-styles-each_example= n[2]
				//Text Styles End
			//Typography Content End
	//Typography Section End
